{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "VaultedTSSDSignal",
  "type": "object",
  "required": [
    "id",
    "timestamp",
    "meta",
    "market",
    "action",
    "price",
    "source",
    "timeframe",
    "score",
    "subscribed"
  ],
  "properties": {
    "id": {
      "type": "string"
    },
    "timestamp": {
      "type": "number",
      "description": "epoch ms"
    },
    "meta": {
      "type": "object",
      "required": [
        "symbol",
        "strategyId",
        "agentId"
      ],
      "properties": {
        "symbol": {
          "type": "string"
        },
        "strategyId": {
          "type": "string"
        },
        "agentId": {
          "type": "string"
        }
      }
    },
    "market": {
      "type": "string",
      "enum": [
        "crypto",
        "forex",
        "stocks",
        "commodities",
        "futures"
      ]
    },
    "action": {
      "type": "string",
      "enum": [
        "buy",
        "sell"
      ]
    },
    "price": {
      "type": "number"
    },
    "source": {
      "type": "string",
      "enum": [
        "manual",
        "tradingview",
        "mcp",
        "bot"
      ]
    },
    "timeframe": {
      "type": "string",
      "enum": [
        "1m",
        "5m",
        "15m",
        "30m",
        "1h",
        "4h",
        "1d",
        "1w"
      ]
    },
    "strength": {
      "type": "string",
      "enum": [
        "low",
        "medium",
        "high",
        "very-high"
      ]
    },
    "pipeline": {
      "type": "object",
      "properties": {
        "submittedBy": {
          "type": "string"
        },
        "enrichedBy": {
          "type": "string"
        },
        "scoredBy": {
          "type": "string"
        }
      }
    },
    "indicators": {
      "type": "array"
    },
    "analysis": {
      "type": "array"
    },
    "patternAnalysis": {
      "type": "object"
    },
    "score": {
      "type": "object",
      "required": [
        "overall"
      ],
      "properties": {
        "overall": {
          "type": "number"
        },
        "technical": {
          "type": "number"
        },
        "fundamental": {
          "type": "number"
        },
        "sentiment": {
          "type": "number"
        },
        "breakdown": {
          "type": "object"
        }
      }
    },
    "riskRewardRatio": {
      "type": "number"
    },
    "takeProfitLevels": {
      "type": "array"
    },
    "stopLoss": {
      "type": "number"
    },
    "subscribed": {
      "type": "boolean"
    }
  },
  "additionalProperties": true
}